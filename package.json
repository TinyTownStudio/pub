            "qs@>=6.5.0 <6.5.3": ">=6.5.3",
            "express@<4.19.2": ">=4.19.2",
            "lodash@<4.17.12": ">=4.17.12",
            "jsonwebtoken@<9.0.0": ">=9.0.0",
            "ajv@<6.12.3": ">=6.12.3",
            "jsonwebtoken@<=8.5.1": ">=9.0.0",
